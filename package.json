{
  "name": "adair",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "element-ui": "^2.12.0",
    "github-markdown-css": "^4.0.0",
    "gojs": "^2.1.33",
    "highlight.js": "^10.1.1",
    "jquery": "^3.5.1",
    "lottie-web": "^5.6.8",
    "vue": "^2.6.11",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.9",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.9",
    "@vue/cli-service": "^3.0.0-rc.9",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.19",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "text-loader": "0.0.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "no-debugger": 0,
      "no-console": 0,
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "ignore",
          "named": "ignore"
        }
      ],
      "no-unused-vars": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
